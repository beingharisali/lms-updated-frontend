{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/components/Header.jsx",
    "src/app/staff/test/page.jsx",
    "src/components/shared/Student.jsx",
    "src/features/students/components/DeleteStudentModal.tsx",
    "src/features/students/components/EditStudentModal.tsx",
    "src/features/students/components/ViewStudentModal.tsx",
    "src/components/table/DataTable.jsx",
    "src/services/student.api.js"
, "src/app/instructor/quizzes/page.tsx", "src/app/admin/fees/[id]/page.jsx", "src/app/admin/fees/[id]/page.tsx"  ],
  "exclude": ["node_modules"]
}
